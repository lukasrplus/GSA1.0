
<div class="content_box">

<h1>Welcome to GSA</h1>

<h2>Please Login</h2>


<%= form_tag sessions_url do %>

  <h3><%= label_tag 'email', 'Email' %></h3>
  <%= text_field_tag 'email'%>

  <h3><%= label_tag 'password', 'Password' %></h3>
  <%= password_field_tag 'password' %>


  <%= submit_tag 'LOGIN' %>

<% end %>

</div>
